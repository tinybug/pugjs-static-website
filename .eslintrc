{
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "jquery": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "rules": {
      "no-console": 0
  }
}
